<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Summary>Restier is a framework for building convention-based, secure, queryable APIs with ASP.NET. This package contains runtime components for interacting with DbContexts from Entity Framework 6.1.3 and later.</Summary>
    <Description>
      $(Summary)

      Commonly used types:
      Microsoft.Restier.EntityFramework.EntityFrameworkApi
    </Description>
    <PackageTags>$(PackageTags)entityframework;entityframework6</PackageTags>

    <TargetFrameworks>net472</TargetFrameworks>
  </PropertyGroup>
  
  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>Microsoft.Restier.Tests.EntityFramework, </_Parameter1>
    </AssemblyAttribute>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>Microsoft.Restier.Tests.AspNet, </_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EntityFramework" Version="[6.2.0, 7.0.0)" />
    <PackageReference Include="Microsoft.OData.Core" Version="[7.8.1, 8.0.0)" />
    <PackageReference Include="Microsoft.OData.Edm" Version="[7.8.1, 8.0.0)" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Restier.Core\Microsoft.Restier.Core.csproj" />
  </ItemGroup>
  
</Project>
